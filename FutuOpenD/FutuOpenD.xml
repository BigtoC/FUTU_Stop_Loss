	<login_account>88888888</login_account>
	<!-- 登录密码明文，密码密文存在情况下只使用密文 -->
	<login_pwd>123456</login_pwd>
	<!-- FutuOpenD语言，en：英文，chs：简体中文 -->
	<lang>chs</lang>
