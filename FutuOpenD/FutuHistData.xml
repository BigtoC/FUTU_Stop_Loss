<futu_hist_data>
	<!-- 0为不下载，非0为下载 -->
	<enable>1</enable>
	<!-- 需要先在FueuOpenD中登录该牛牛号，才能下载历史数据 -->
	<nnid>10000</nnid>
	<!-- 时间格式为HHMM，每天到当地这个时间点开始下载，例如0800表示08:00 -->
	<begin_time>0800</begin_time>
	<!-- 时间格式为HHMM，每天到当地这个时间点暂停下载，end小于begin时表示end为第二天。例如2300表示23:00 -->
	<end_time>2300</end_time>
	<!-- 可选市场类型为hk、hk_future（港股期货）, us、cn，多个市用|连接，如hk|cn|us|hk_future -->
	<market>hk</market>
	<!-- 可选的K线类型为day、week、month、min1、min5、min15、min30、min60，多个类型用|连接，如day|week|month|year|min1|min5|min15|min30|min60 -->
	<kline_type>day</kline_type>
	<!-- 自定义历史数据存放位置 -->
	<hist_data_dir>D:\ft_hist_data</hist_data_dir>
</futu_hist_data>